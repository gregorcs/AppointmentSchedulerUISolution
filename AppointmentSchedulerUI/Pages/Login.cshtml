@page
@model AppointmentSchedulerUI.Pages.NewFolder.LoginModel
@{
}

<head>
    <link rel="stylesheet" href="~/css/Login.css" type="text/css" />
</head>
<div class="login-container">
    <form method="post">
        <div class="login-row">
            <label asp-for="Credential.UserName"></label>
            <input asp-for="Credential.UserName" />
        </div>
        <div class="login-row">
            <label asp-for="Credential.Password"></label>
            <input asp-for="Credential.Password" type="password" />
        </div>
        <div class="login-row">
            <input type="submit" />
        </div>
    </form>
</div>